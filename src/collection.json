{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "workspace": {
      "description": "A new Platform workspace.",
      "factory": "./workspace",
      "schema": "./workspace/schema.json",
      "hidden": true
    },
    "app": {
      "aliases": ["application"],
      "description": "A new Angular application.",
      "factory": "./app",
      "schema": "./app/schema.json"
    },
    "docs": {
      "aliases": ["documentation", "doc"],
      "description": "A new Angular documentation application",
      "factory": "./docs",
      "schema": "./docs/schema.json"
    },
    "platform": {
      "aliases": ["plat"],
      "description": "Initialize a new Platform workspace.",
      "factory": "./platform",
      "schema": "./platform/schema.json"
    },
    "server": {
      "description": "A new .NET Web Server",
      "factory": "./server",
      "schema": "./server/schema.json"
    }
  }
}
