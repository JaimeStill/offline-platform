{
  "name": "<%= dasherize(name) %>",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "build": "ng build <%= dasherize(library) %> --configuration development",
    "restore": "dotnet restore ./<%= dasherize(serverRoot) %> -s ./<%= dasherize(serverRoot) %>/nuget-packages",
    "seed": "dotnet run --project ./<%= dasherize(serverRoot) %>/dbseeder -- \"Server=.\\DevSql;Database=<%= dasherize(name) %>-dev;Trusted_Connection=True;\"",
    "start:server": "dotnet run --project ./<%= dasherize(serverRoot) %>/<%= classify(server) %>.Web",
    "update-db": "dotnet ef database update -s ./<%= dasherize(serverRoot) %>/<%= classify(server)%>.Web",
    "watch": "ng build <%= dasherize(library) %> --watch --configuration development"
  },
  "dependencies": {
    "@angular/animations": "^12.2.0",
    "@angular/cdk": "^12.2.0",
    "@angular/common": "^12.2.0",
    "@angular/compiler": "^12.2.0",
    "@angular/core": "^12.2.0",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "^12.2.0",
    "@angular/material": "^12.2.0",
    "@angular/platform-browser": "^12.2.0",
    "@angular/platform-browser-dynamic": "^12.2.0",
    "@angular/router": "^12.2.0",
    "@microsoft/signalr": "^5.0.8",
    "hammerjs": "^2.0.8",
    "marked": "^2.1.3",
    "prismjs": "^1.24.1",
    "rxjs": "6.5.3",
    "tslib": "^2.3.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.0",
    "@angular/cli": "^12.2.0",
    "@angular/compiler-cli": "^12.2.0",
    "@angular/language-service": "^12.2.0",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@types/marked": "^2.0.4",
    "@types/node": "^16.4.13",
    "@types/prismjs": "^1.16.6",
    "esbuild-windows-64": "^0.12.19",
    "ng-packagr": "^12.2.0",
    "prism-themes": "^1.8.0",
    "roboto-fontface": "^0.10.0",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5"
  }
}
