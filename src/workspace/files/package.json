{
  "name": "<%= dasherize(name) %>",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "build": "ng build <%= dasherize(library) %> --configuration development",
    "restore": "dotnet restore ./<%= dasherize(serverRoot) %> -s ./<%= dasherize(serverRoot) %>/nuget-packages",
    "seed": "dotnet run --project ./<%= dasherize(serverRoot) %>/dbseeder -- \"Server=.\\DevSql;Database=<%= dasherize(name) %>-dev;Trusted_Connection=True;\"",
    "start:server": "dotnet run --project ./<%= dasherize(serverRoot) %>/<%= classify(server) %>.Web",
    "update-db": "dotnet ef database update -s ./<%= dasherize(serverRoot) %>/<%= classify(server)%>.Web",
    "watch": "ng build <%= dasherize(library) %> --watch --configuration development",
    "watch:server": "dotnet watch run --project ./<%= dasherize(serverRoot) %>/<%= classify(server) %>.Web"
  },
  "dependencies": {
    "@angular/animations": "^14.0.1",
    "@angular/cdk": "^14.0.1",
    "@angular/common": "^14.0.1",
    "@angular/compiler": "^14.0.1",
    "@angular/core": "^14.0.1",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "^14.0.1",
    "@angular/material": "^14.0.1",
    "@angular/platform-browser": "^14.0.1",
    "@angular/platform-browser-dynamic": "^14.0.1",
    "@angular/router": "^14.0.1",
    "@microsoft/signalr": "^6.0.5",
    "hammerjs": "^2.0.8",
    "marked": "^4.0.16",
    "prismjs": "^1.28.0",
    "rxjs": "^7.5.5",
    "tslib": "^2.4.0",
    "zone.js": "^0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.1",
    "@angular/cli": "^14.0.1",
    "@angular/compiler-cli": "^14.0.1",
    "@angular/language-service": "^14.0.1",
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@types/marked": "^4.0.3",
    "@types/node": "^17.0.41",
    "@types/prismjs": "^1.26.0",
    "esbuild-windows-64": "^0.14.43",
    "ng-packagr": "^14.0.1",
    "prism-themes": "^1.9.0",
    "roboto-fontface": "^0.10.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  }
}
