{
  "name": "<%= dasherize(name) %>",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "build": "ng build <%= dasherize(library) %> --configuration development",
    "restore": "dotnet restore ./<%= dasherize(serverRoot) %> -s ./<%= dasherize(serverRoot) %>/nuget-packages",
    "seed": "dotnet run --project ./<%= dasherize(serverRoot) %>/dbseeder -- \"Server=.\\DevSql;Database=<%= dasherize(name) %>-dev;Trusted_Connection=True;\"",
    "start:server": "dotnet run --project ./<%= dasherize(serverRoot) %>/<%= classify(server) %>.Web",
    "update-db": "dotnet ef database update -s ./<%= dasherize(serverRoot) %>/<%= classify(server)%>.Web",
    "watch": "ng build <%= dasherize(library) %> --watch --configuration development",
    "watch:server": "dotnet watch run --project ./<%= dasherize(serverRoot) %>/<%= classify(server) %>.Web"
  },
  "dependencies": {
    "@angular/animations": "^13.1.0",
    "@angular/cdk": "^13.1.0",
    "@angular/common": "^13.1.0",
    "@angular/compiler": "^13.1.0",
    "@angular/core": "^13.1.0",
    "@angular/flex-layout": "^13.0.0-beta.36",
    "@angular/forms": "^13.1.0",
    "@angular/material": "^13.1.0",
    "@angular/platform-browser": "^13.1.0",
    "@angular/platform-browser-dynamic": "^13.1.0",
    "@angular/router": "^13.1.0",
    "@microsoft/signalr": "^6.0.0",
    "hammerjs": "^2.0.8",
    "marked": "^4.0.7",
    "prismjs": "^1.25.0",
    "rxjs": "^7.4.0",
    "tslib": "^2.3.1",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.1.1",
    "@angular/cli": "^13.1.1",
    "@angular/compiler-cli": "^13.1.0",
    "@angular/language-service": "^13.1.0",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@types/marked": "^4.0.1",
    "@types/node": "^16.11.12",
    "@types/prismjs": "^1.16.6",
    "esbuild-windows-64": "^0.12.19",
    "ng-packagr": "^13.1.1",
    "prism-themes": "^1.9.0",
    "roboto-fontface": "^0.10.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.3"
  }
}
