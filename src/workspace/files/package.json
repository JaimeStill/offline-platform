{
  "name": "<%= dasherize(name) %>",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "build": "ng build <%= dasherize(library) %> --configuration development",
    "e2e-merge": "mochawesome-merge <%= clientRoot %>/**/cypress/results/**/*.json -o report/report.json",
    "e2e-generate": "marge report/report.json -o report -f report --charts",
    "restore": "dotnet restore ./<%= dasherize(serverRoot) %>",
    "seed": "dotnet run --project ./<%= dasherize(serverRoot) %>/<%= classify(server) %>.DbCli -- \"Dev\"",
    "start-server": "dotnet run --project ./<%= dasherize(serverRoot) %>/<%= classify(server) %>.Web",
    "update-db": "dotnet ef database update -s ./<%= dasherize(serverRoot) %>/<%= classify(server)%>.Web",
    "watch": "ng build <%= dasherize(library) %> --watch --configuration development",
    "watch-server": "dotnet watch run --project ./<%= dasherize(serverRoot) %>/<%= classify(server) %>.Web"
  },
  "dependencies": {
    "@angular/animations": "^14.1.1",
    "@angular/cdk": "^14.1.1",
    "@angular/common": "^14.1.1",
    "@angular/compiler": "^14.1.1",
    "@angular/core": "^14.1.1",
    "@angular/flex-layout": "^14.0.0-beta.40",
    "@angular/forms": "^14.1.1",
    "@angular/material": "^14.1.1",
    "@angular/platform-browser": "^14.1.1",
    "@angular/platform-browser-dynamic": "^14.1.1",
    "@angular/router": "^14.1.1",
    "@microsoft/signalr": "^6.0.7",
    "hammerjs": "^2.0.8",
    "marked": "^4.0.18",
    "prismjs": "^1.28.0",
    "rxjs": "^7.5.6",
    "tslib": "^2.4.0",
    "zone.js": "^0.11.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.1",
    "@angular/cli": "^14.1.1",
    "@angular/compiler-cli": "^14.1.1",
    "@angular/language-service": "^14.1.1",
    "@fortawesome/fontawesome-free": "^6.1.2",
    "@types/marked": "^4.0.3",
    "@types/node": "^18.6.4",
    "@types/prismjs": "^1.26.0",
    "cypress": "^10.4.0",
    "esbuild-windows-64": "^0.14.53",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "ng-packagr": "^14.1.0",
    "prism-themes": "^1.9.0",
    "roboto-fontface": "^0.10.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
